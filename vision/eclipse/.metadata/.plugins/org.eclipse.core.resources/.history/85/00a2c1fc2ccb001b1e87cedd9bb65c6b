
#include <stdio.h>
#include <string.h>

#include "alt_types.h"
#include "io.h"
#include "system.h"
#include "altera_avalon_pio_regs.h"

int main() {
  printf("Hello from Nios II!\n");
  TransData msg;
loop: // Is this necessary - interrupt registered?
  while (!IOADDR_ALTERA_AVALON_PIO_DATA(HANDSHAKE_BASE));
  __handle_handshake_irq(&msg);
  goto loop;
}
